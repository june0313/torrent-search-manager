{
  "manifest_version": 2,

  "name": "Torrent Search Manager",
  "description": "토랜트 파일을 검색하기 위한 검색어 관리 기능을 제공합니다.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "resources/icon.png",
    "default_popup": "dist/popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}